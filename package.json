    "webpack-node-externals": "^1.7.2",
    "webpack-notifier": "^1.7.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --watch configs --exec \"node build/app.js\"",
    "dev:build-server": "nodemon --watch configs --exec \"webpack --config configs/webpack.server.js --watch\"",
    "dev:build-client": "nodemon --watch configs --exec \"webpack --config configs/webpack.client.js --watch\"",
    "prod": "NODE_ENV=production npm-run-all --parallel prod:*",
    "prod:build-server": "NODE_ENV=production webpack --config configs/webpack.server.js",
    "prod:build-client": "NODE_ENV=production webpack --config configs/webpack.client.js"
  }
